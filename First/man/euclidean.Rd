## The function is currently defined as
function (a, b) 
{
    stopifnot(is.numeric(a) | length(a) > 1 & is.numeric(b) | 
        length(b) > 1)
    vect <- c(a, b)
    a <- max(vect)
    b <- min(vect)
    q <- 1
    r0 <- 1
    while (r0 != 0) {
        while ((q) * b <= a) {
            q <- q + 1
        }
        q <- q - 1
        r0 <- a - q * b
        a <- b
        b <- r0
        q <- 1
    }
    return(a)
  }
}
